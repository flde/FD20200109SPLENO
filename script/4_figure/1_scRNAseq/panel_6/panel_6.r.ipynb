   "source": [
    "# Density plot "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "24121109-8af5-4292-8af4-c03858aaec7c",
   "metadata": {},
   "outputs": [],
   "source": [
    "so <- AddMetaData(so, read.csv(\"result/pseudotime/ery_dpt.csv\", row.names=1))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "4c7b0e7f-799d-429c-b8ed-03490ccecca8",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Min max scale per condition \n",
    "data <- so@meta.data\n",
    "\n",
    "minmax_scale <- function(x) {(x - min(x, na.rm=TRUE)) / (max(x, na.rm=TRUE) - min(x, na.rm=TRUE))}\n",
    "data <- data %>% dplyr::filter(!is.na(dpt_pseudotime)) %>% dplyr::group_by(sample_group) %>% dplyr::mutate(dpt_pseudotime=minmax_scale(dpt_pseudotime)) %>% dplyr::ungroup() %>% as.data.frame() "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "27602dbd-7d68-4ed0-a005-5380e98339a9",
   "metadata": {},
   "outputs": [],
   "source": [
    "cnt <- GetAssayData(so, assay=\"RNA\", slot=\"data\")\n",
    "data <- cnt[\"Erfe\", ] %>% data.frame() %>% dplyr::rename(\"Erfe\"=\".\") %>% tibble::rownames_to_column(\"cell_id\") %>% dplyr::left_join(data, ., by=join_by(cell_id))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "d1d04e7e-4296-4e96-9aff-2e2201a6fdd3",
   "metadata": {},
   "outputs": [],
   "source": [
    "dpt_range <- range(data$dpt_pseudotime, na.rm=TRUE)\n",
    "color_gradient <- c(color$celltype_low[\"MEP\"], color$celltype_low[\"MEP\"], color$celltype_low[\"Proerythroblast\"], color$celltype_low[\"Proerythroblast\"], color$celltype_low[\"Erythroblast\"], color$celltype_low[\"Erythroblast\"])\n",
    "breaks_gradient <- scales::rescale(c(0, 0.4, 0.4, 0.8, 0.8, 1), from=dpt_range)\n",
    "\n",
    "rp_1 <- lapply(c(\"D0\", \"D1\", \"D3\"), function(i) {\n",
    "\n",
    "    ggplot(data[data$dpi==i, ], aes(x=dpt_pseudotime, y=dpi)) +\n",
    "  \n",
    "        ggridges::geom_density_ridges_gradient(aes(fill=after_stat(x)), scale=1, rel_min_height=0, from=0, to=1, panel_scaling=FALSE, bandwidth=0.05, linewidth=unit(0.5/2.141959, \"pt\")) +\n",
    "        xlim(c(0, 1)) + \n",
    "        scale_fill_gradientn(colours=color_gradient, values=breaks_gradient, limits=dpt_range, na.value=\"#000000\", guide=\"none\") + \n",
    "        facet_grid(~genotype) + \n",
    "        scale_x_continuous(breaks=c(0.0, 0.5, 1.0), labels=c(\"0.0\", \"0.5\", \"1.0\"), limits=c(0, 1), expand=c(0, 0)) +\n",
    "        scale_y_discrete(expand=c(0, 0)) +\n",
    "        theme_global_set(4) + \n",
    "        theme(strip.text=element_blank(), strip.background = element_blank())\n",
    "    \n",
    "}\n",
    "      )"
   ]
  },
  {
