   "source": [
    "# Cell cycle phase \n",
    "tf_color <- c(\n",
    "    \n",
    "    \"#ffcdb2\", \"#ffb4a2\", \"#e5989b\", \"#b5838d\", \"#6d6875\", \"#cfe1b9\", \"#97a97c\", \"#718355\", \"#c0d6df\", \"#474973\", \"#4f6d7a\", \"#4a6fa5\", \"#161b33\", \"#0d0c1d\", \"#7f7f7f\", \"#535353\"\n",
    "    \n",
    ")\n",
    "\n",
    "names(tf_color) <- c(\n",
    "    \n",
    "    \"Stat1\", \"Stat2\", \"Stat3\", \"Irf1\", \"Irf7\", \"Spi1\", \"Ikzf2\", \"Gata2\", \"Myc\", \"Gata1\", \"Nfia\", \"Klf1\", \"E2f4\", \"E2f2\", \"Other\", \"None\"\n",
    "    \n",
    ")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "01ef4da7-563e-4579-9e5f-b73076c2f1b0",
   "metadata": {},
   "outputs": [],
   "source": [
    "dp_tf <- function(pt_split_ref, tf_tg_mat_ref) {\n",
    "\n",
    "    pt_split_ref <- pt_split_ref[names(pt_split_ref) %in% rownames(tf_tg_mat_ref)]\n",
    "    \n",
    "    tf_tg_mat_ref <- tf_tg_mat_ref[, colSums(tf_tg_mat_ref)>0]\n",
    "    tf_tg_mat_ref <- tf_tg_mat_ref[names(pt_split_ref), ]\n",
    "\n",
    "    tf_tg_mat_ref$None <- ifelse(rowSums(tf_tg_mat_ref)==0, 1, 0)\n",
    "    \n",
    "    data <- tf_tg_mat_ref %>% dplyr::mutate(group=pt_split_ref) %>% aggregate(. ~ group, ., sum) %>% tidyr::pivot_longer(cols=-group, names_to=\"TF\", values_to=\"counts\") %>% \n",
    "        dplyr::mutate(TF=ifelse(TF %in% names(tf_color), TF, \"Other\")) %>% \n",
    "        # dplyr::group_by(group, TF) %>% dplyr::summarise(counts=sum(counts)) %>% dplyr::mutate(ratio=counts/sum(counts)) %>% dplyr::mutate(TF=ifelse(ratio>0.01, TF, \"Other\")) %>% \n",
    "        dplyr::group_by(group, TF) %>% dplyr::summarise(counts=sum(counts)) %>% dplyr::mutate(ratio=counts/sum(counts)) \n",
    "    \n",
    "    data <- split(data, f=data$group)\n",
    "    data <- lapply(data, function(x) {\n",
    "        \n",
    "        x %>% dplyr::arrange(dplyr::desc(ratio)) %>% \n",
    "            dplyr::mutate(TF=factor(TF, levels=names(tf_color))) %>% dplyr::arrange(dplyr::desc(TF)) %>% \n",
    "            dplyr::mutate(label_pos_y=cumsum(ratio)-0.5*ratio)\n",
    "        \n",
    "    }\n",
    "                  )\n",
    "    \n",
    "    bp_1 <- lapply(data, function(x) {\n",
    "        \n",
    "        ggplot(x, aes(x=2, y=ratio, fill=TF)) + geom_bar(stat=\"identity\", width=1) + \n",
    "        # geom_text(aes(y=label_pos_y, label=paste0(round(ratio*100), \"%\")), color=\"black\", size=1.5) +\n",
    "        coord_polar(theta=\"y\", start=0) + xlim(0.5, 2.5) + \n",
    "        scale_fill_manual(values=tf_color)  + \n",
    "        guides(fill=guide_legend(ncol=1, override.aes=list(alpha=1, size=1.5), keywidth=0.25, keyheight=0.25, default.unit=\"cm\")) + \n",
    "        theme_global_set(4)\n",
    "    \n",
    "    }\n",
    "                  )\n",
    "    \n",
    "    return(bp_1)\n",
    "    \n",
    "}"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "52556e9d-3269-4ce9-b984-5c95ca5a5aa5",
